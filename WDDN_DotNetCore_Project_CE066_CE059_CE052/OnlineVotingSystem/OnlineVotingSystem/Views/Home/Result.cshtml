@model IEnumerable<OnlineVotingSystem.Models.Result>

<h1 class="h2 text-center badge-secondary">Result list</h1>
<table class="table table-bordered border-primary">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ElectionName)
            </th>
            @*<th>
                @Html.DisplayNameFor(model => model.EndDate)
            </th>*@
            <th>
                @Html.DisplayNameFor(model => model.WinnerName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Votes)
            </th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.ElectionName)
            </td>
            @*<td>
                @Html.DisplayFor(modelItem => item.EndDate)
            </td>*@
            <td>
                @Html.DisplayFor(modelItem => item.WinnerName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Votes)
            </td>
        </tr>
}
    </tbody>
</table>